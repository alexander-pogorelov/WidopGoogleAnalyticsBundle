{
    "name": "rdey/google-analytics-bundle",
    "description": "Google certificate-based authentication in server-to-server interactions with google analytics",
    "keywords": [ "google", "analytics", "server" ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Widop",
            "email": "contact@widop.com"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php-http/client-common": "^1.5",
        "php-http/discovery": "^1.0",
        "php-http/httplug": "^1.0",
        "rdey/google-analytics": "dev-http-client as 4.0.0",
        "symfony/config": "^3.0|^4.0",
        "symfony/dependency-injection": "^3.0|^4.0",
        "symfony/http-kernel": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Widop\\GoogleAnalyticsBundle": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "Widop/GoogleAnalyticsBundle",
    "extra": {
        "branch-alias": {
            "dev-dev": "3.0.x-dev"
        }
    }
}
